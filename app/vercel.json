{
  "version": 2,
  "builds": [
    {
      "src": "index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/favicon.ico",
      "dest": "/favicon.ico"
    },
    {
      "src": "/chill.mp3",
      "dest": "/chill.mp3"
    },
    {
      "src": "/grass.gif",
      "dest": "/grass.gif"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
